{
  "TCP": {
    "443": {
      "HTTPS": true
    }
  },
  "Web": {
    "${TS_CERT_DOMAIN}:443": {
      "Handlers": {
        "/api/": {
          "Proxy": "http://ephemeral-environments-api:3000"
        },
        "/": {
          "Proxy": "http://ephemeral-environments-frontend:80"
        }
      }
    }
  },
  "AllowFunnel": {
    "${TS_CERT_DOMAIN}:443": false
  }
}
